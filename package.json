{
    "main": "index.js",
    "scripts": {
        "lint": "eslint ./src/**/*.{ts,tsx}",
        "build": "tsc --noEmit && parcel build src/index.html --public-url ./ --no-source-maps",
        "postbuild": "node scripts/generate-index",
        "start": "concurrently 'yarn parcel' 'yarn server'",
        "parcel": "parcel src/index.html",
        "server": "node proxy/server.js",
        "test": "jest --config jest.config.js --watchAll",
        "test:w": "jest --config jest.config.js --watch"
    },
    "jest": {
        "preset": "ts-jest",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "testMatch": [
            "**/__tests__/*.(test|spec).(ts|tsx|js)"
        ]
    },
    "devDependencies": {
        "@types/enzyme": "^3.10.5",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.4",
        "@types/react": "^16.9.38",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "concurrently": "^5.2.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.5.0",
        "eslint": "^6.8.0",
        "jest": "^26.1.0",
        "jest-fetch-mock": "^3.0.3",
        "parcel-bundler": "^1.12.4",
        "ts-jest": "^26.1.1",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@material-ui/core": "^4.10.2",
        "axios": "^0.19.2",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "path": "^0.12.7",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0"
    }
}
